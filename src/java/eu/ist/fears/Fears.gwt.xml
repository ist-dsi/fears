<module rename-to='fears'>
    
    <!-- Inherit the core Web Toolkit stuff.                  -->
    <inherits name='com.google.gwt.user.User'/>
    
    <!-- Specify the app entry point class.                   -->
    <entry-point class='eu.ist.fears.client.Fears'/>
    <entry-point class='eu.ist.fears.client.admin.Admin'/>
    
    <!-- Servlet: -->
    <servlet path='/service' class='eu.ist.fears.server.FearsServiceImpl'/>
    
    <stylesheet src="../general.css"/>
    
    <source path="client"/>
    <!-- Add the common dir -->
    <source path="common"/>
    
</module>